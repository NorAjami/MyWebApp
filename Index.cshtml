@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Nors Konst – Street Art & Oljemålningar</title>

    <!-- CSS för design och animationer -->
    <style>
        /* Grundläggande stil för hela sidan */
        body {
            font-family: "Consolas", "Courier New", monospace;
            background-color: black; /* Svart bakgrund */
            color: white; /* Vit text */
            text-align: center;
            padding: 20px;
        }

        /* Stil för navigationsmenyn */
        nav {
            background-color: #222;
            padding: 10px;
            position: fixed;
            top: 0;
            width: 100%;
            left: 0;
        }
        nav a {
            color: white;
            text-decoration: none;
            margin: 15px;
            font-size: 18px;
            font-weight: bold;
        }
        nav a:hover {
            color: yellow;
        }

        /* Stil för huvudrubrik */
        h1 {
            font-size: 36px;
            margin-top: 80px;
        }

      
       

        /* Knappstil */
        .button {
            display: inline-block;
            padding: 12px 24px;
            background: yellow;
            color: black;
            text-decoration: none;
            font-weight: bold;
            border-radius: 5px;
            margin-top: 20px;
        }
        .button:hover {
            background: orange;
        }
    </style>
</head>
<body>

    <!-- Navigationsmeny -->
    <nav>
        <a href="index.html">Hem</a>
        <a href="about.html">Om mig</a>
        <a href="gallery.html">Galleri</a>
        <a href="workshops.html">Workshops</a>
        <a href="contact.html">Kontakt</a>
    </nav>

    <h1>Välkommen till Nors Konst!</h1>
    <p>Utforska min värld av street art, oljemålningar och kreativa workshops.</p>

    <!-- Bild från Cloud Storage -->
    <img src="https://storage.cloud.google.com/norscat/lolcat.jpg" class="rotate" alt="LOL Cat">

    <!-- Knapp till Galleri -->
    <a href="gallery.html" class="button">Se mitt galleri</a>

</body>
</html>@{
